{
  "name": "mcp-stack",
  "version": "0.1.3",
  "description": "MCP Stack Manager for Claude Code - manage MCP server configurations with stack templates",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-stack": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "claude",
    "claude-code",
    "model-context-protocol",
    "cli",
    "developer-tools"
  ],
  "author": "seenu30",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seenu30/mcp-stack-manager.git"
  },
  "homepage": "https://github.com/seenu30/mcp-stack-manager#readme",
  "bugs": {
    "url": "https://github.com/seenu30/mcp-stack-manager/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@inquirer/checkbox": "^5.0.7",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.23",
    "ora": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.14.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
